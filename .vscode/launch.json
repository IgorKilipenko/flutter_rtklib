{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "inputs": [
        {
            "type": "promptString",
            "id": "libsPath",
            "description": "Libs direcotory for fast tests",
            "default": "${workspaceFolder}/example/build/windows/runner/Debug"
        }
    ],
    "configurations": [
        {
            "name": "flutter_rtklib",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_rtklib (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_rtklib (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "example",
            "cwd": "example",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "example (profile mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "example (release mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "example__device_tests",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}/example",
            "windows": {
                
            },
            "runTestsOnDevice": true,
            "program": "test/widget_test.dart"
        },
        {
            "name": "rtklib__device_tests",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}",
            "windows": {
                
            },
            "runTestsOnDevice": true,
            "program": "test/flutter_rtklib_test.dart"
        },
        {
            "name": "example__fast_tests",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}/example",
            "windows": {
                
            },
            "env": {
                "LIBSERIALPORT_PATH": "${input:libsPath}",
                "RTKLIB_PATH": "${input:libsPath}"
            },
            "program": "test"
        },
        {
            "name": "rtklib__fast_tests",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}",
            "windows": {
                
            },
            "env": {
                "LIBSERIALPORT_PATH": "${input:libsPath}",
                "RTKLIB_PATH": "${input:libsPath}"
            },
            "program": "test"
        },
        {
            "type": "dart",
            "name": "vscode-jest-tests.v2",
            "request": "launch",
            "program": "test",
            "args": [
                "test",
                "--dart-define=LIBSERIALPORT_PATH=${workspaceFolder}/example/build/windows/runner/Debug",
                "--dart-define=RTKLIB_PATH=${workspaceFolder}/example/build/windows/runner/Debug",
            ],
            "cwd": "${workspaceFolder}",
            //"console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "disableOptimisticBPs": true,
            /*"windows": {
              "program": "${workspaceFolder}/node_modules/jest/bin/jest"
            }*/
          }
    ]
}