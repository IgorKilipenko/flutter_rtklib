    "version": "0.2.0",
    "inputs": [
        {
            "type": "promptString",
            "id": "libsPath",
            "description": "Libs direcotory for fast tests",
            "default": "${workspaceFolder}/test/.libs/"
        }
    ],
    "configurations": [
        {
            "name": "flutter_rtklib",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_rtklib (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_rtklib (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "example",
            "cwd": "example",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "example (profile mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "example (release mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "rtklib__device_tests",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}",
            "windows": {
                
            },
            "runTestsOnDevice": true,
            "program": "test/flutter_rtklib_test.dart"
        },
        {
            "name": "example__fast_tests",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}/example",
            "windows": {
                
            },
            "env": {
                "LIBSERIALPORT_PATH": "${input:libsPath}",
                "RTKLIB_PATH": "${input:libsPath}"
            },
            "program": "test"
        },
        {
            "name": "rtklib__fast_tests",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}",
            "windows": {
                
            },
            "env": {
                "LIBSERIALPORT_PATH": "${input:libsPath}",
                "RTKLIB_PATH": "${input:libsPath}"
            },
            "program": "test"
        },
    ]
}